RewriteEngine On
RewriteBase /SpaVita-equipe5/public/

# Si le fichier ou le dossier demandé existe, on le sert directement
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Sinon, on redirige toutes les requêtes vers index.php avec le paramètre page
RewriteRule ^([^/]+)$ index.php?page=$1 [QSA,L]
